              <stringProp name="script">import groovy.json.JsonSlurper;

def failureMessage = &quot;&quot;;
def jsonResponse = null;

JsonSlurper JSON = new JsonSlurper();

try 
{
	jsonResponse = JSON.parseText(prev.getResponseDataAsString());
} 
catch (Exception e)
{	
	failureMessage += &quot;\n------------------ RESPONSE BODY --------------- \n\n&quot;
	failureMessage += &quot;Ошибка: тело ответа пустое. \n&quot;
}


if (!&quot;202&quot;.equals(prev.getResponseCode()))
{
	failureMessage += &quot;\n------------------ RESPONSE CODE --------------- \n\n&quot;
	failureMessage += &quot;Ожидаемый [202] \nФактический [&quot; + prev.getResponseCode() + &quot;]\n\n&quot;;

	failureMessage += &quot; uid статьи : &quot; + jsonResponse.uid + &quot;]\n\n&quot;;
	failureMessage += &quot; uid статьи : &quot; + prev.getRequestHeaders() + &quot;]\n\n&quot;;
}

if (failureMessage?.trim())
{
	AssertionResult.setFailureMessage(failureMessage);
	AssertionResult.setFailure(true);
}


</stringProp>
